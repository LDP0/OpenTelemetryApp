{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "OpenTelemetryAppServiceOne",
    "OtlpTracesEndpoint": "http://localhost:4317",
    "OtlpMetricsEndpoint": "http://localhost:4318",
    "EnableConsoleExporter": true,
    "Environment": "development",
    "Version": "1.0.0",
    "Region": "za",
    "CustomAttributes": {
      "custom.attribute": "customValueServiceOne"
    },
    "Sampler": "AlwaysOn",
    "SampleRate": 1.0,
    "EnableTraces": true,
    "EnableMetrics": true
  }
}